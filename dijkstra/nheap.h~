
#include <vector>


class NHeap{
private:
	std::vector< std::pair<int,int> > heap;
	std::map<int, int> pos_heap;  	
	int n;

	void heapify_up(int key);
	void heapify_down(int key);

public:
	NHeap(int n, int key, int e);
	void insert(int key, int e);
	void update(int key, ne);
	void deletemin();

};
